{
  "name": "metadata-quality-react",
  "version": "1.1.0",
  "description": "React application for evaluating RDF metadata quality based on FAIR+C principles",
  "homepage": "https://mjanez.github.io/metadata-quality-react/",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/mjanez/metadata-quality-react.git"
  },
  "keywords": [
    "metadata",
    "quality",
    "assessment",
    "rdf",
    "fair",
    "dcat-ap",
    "dcat-ap-es",
    "nti-risp",
    "react",
    "typescript"
  ],
  "dependencies": {
    "@rdfjs/data-model": "^2.1.1",
    "@rdfjs/dataset": "^2.0.2",
    "@rdfjs/types": "^2.0.1",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.126",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@types/react-router-dom": "^5.3.3",
    "binary-extensions": "^3.1.0",
    "bootstrap": "^5.3.7",
    "chart.js": "^4.5.0",
    "d3": "^7.9.0",
    "fflate": "^0.8.2",
    "i18next": "^23.16.8",
    "i18next-browser-languagedetector": "^7.2.2",
    "n3": "^1.26.0",
    "rdfxml-streaming-parser": "^3.1.0",
    "react": "^19.1.1",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^19.1.1",
    "react-i18next": "^13.5.0",
    "react-router-dom": "^7.8.2",
    "react-scripts": "5.0.1",
    "react-tooltip": "^5.29.1",
    "shacl-engine": "^1.0.2",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "start": "react-scripts start",
    "start-local": "set BROWSER=none && set PORT=3000 && react-scripts start",
    "build": "cross-env PUBLIC_URL=/metadata-quality-react react-scripts build",
    "build:docker": "react-scripts build",
    "build-local": "npm run build && npx serve -s build -l 8000",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/d3": "^7.4.3",
    "@types/d3-dispatch": "^3.0.5",
    "@types/n3": "^1.26.0",
    "cross-env": "^10.0.0",
    "gh-pages": "^6.3.0",
    "source-map-explorer": "^2.5.3"
  }
}

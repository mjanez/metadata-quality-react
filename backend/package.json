{
  "name": "mqa-backend",
  "version": "2.0.0",
  "description": "Backend server for Metadata Quality Analysis - API & Development support",
  "main": "dist/server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "start:ts": "ts-node server.ts",
    "dev:ts": "nodemon --exec ts-node server.ts",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "lint": "eslint . --ext .ts,.js",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "axios": "^1.6.2",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "n3": "^1.26.0",
    "rdfxml-streaming-parser": "^3.1.0",
    "shacl-engine": "^1.0.2",
    "@rdfjs/data-model": "^2.1.1",
    "@rdfjs/dataset": "^2.0.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "typescript": "^5.3.3",
    "ts-node": "^10.9.2",
    "@types/node": "^20.10.5",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/morgan": "^1.9.9",
    "@types/n3": "^1.16.4"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "metadata",
    "quality",
    "rdf",
    "dcat-ap",
    "shacl",
    "fair",
    "api",
    "backend"
  ],
  "author": "MQA Team",
  "license": "AGPL-3.0"
}
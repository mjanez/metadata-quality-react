          "query": "PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>\nPREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>\nPREFIX dcat: <http://www.w3.org/ns/dcat#>\nPREFIX dct: <http://purl.org/dc/terms/>\nPREFIX dc: <http://purl.org/dc/terms/>\nPREFIX time: <http://www.w3.org/2006/time#>\nPREFIX schema: <http://schema.org/>\nPREFIX foaf: <http://xmlns.com/foaf/0.1/>\n\nCONSTRUCT {\n  # Catalog\n  <http://datos.gob.es/catalog/{org_dir3}> a dcat:Catalog ;\n    dct:title \"Catálogo de datos - {org_dir3}\" ;\n    dct:description \"Catálogo de datasets del organismo {org_dir3}\" ;\n    dct:publisher ?publisher ;\n    dcat:dataset ?dataset ;\n    dct:language \"es\" ;\n    foaf:homepage <http://datos.gob.es> .\n    \n  # Datasets - Basic properties that are most likely to exist\n  ?dataset a dcat:Dataset ;\n    dct:title ?title ;\n    dct:description ?description ;\n    dct:publisher ?publisher ;\n    dcat:theme ?theme ;\n    dcat:keyword ?keyword ;\n    dct:identifier ?identifier ;\n    dct:issued ?issued ;\n    dct:modified ?modified ;\n    dct:language ?language ;\n    dct:license ?license ;\n    dcat:distribution ?distribution .\n    \n  # Distributions - Basic properties\n  ?distribution a dcat:Distribution ;\n    dct:title ?distTitle ;\n    dct:description ?distDescription ;\n    dcat:downloadURL ?downloadURL ;\n    dcat:accessURL ?accessURL ;\n    dcat:mediaType ?mediaType ;\n    dct:format ?format ;\n    dct:license ?distLicense .\n}\nWHERE {\n  # Find datasets for the organization - Try different patterns\n  {\n    # Pattern 1: Direct publisher match\n    ?dataset a dcat:Dataset .\n    ?dataset dct:publisher ?publisher .\n    FILTER (regex(str(?publisher), \"{org_dir3}\", \"i\"))\n  }\n  UNION\n  {\n    # Pattern 2: Catalog -> Dataset relationship\n    ?catalog a dcat:Catalog .\n    ?catalog dcat:dataset ?dataset .\n    ?catalog dct:publisher ?publisher .\n    FILTER (regex(str(?publisher), \"{org_dir3}\", \"i\"))\n  }\n  \n  # Dataset properties - Only include commonly available properties\n  OPTIONAL { ?dataset dct:title ?title }\n  OPTIONAL { ?dataset dct:description ?description }\n  OPTIONAL { ?dataset dcat:theme ?theme }\n  OPTIONAL { ?dataset dcat:keyword ?keyword }\n  OPTIONAL { ?dataset dct:identifier ?identifier }\n  OPTIONAL { ?dataset dct:issued ?issued }\n  OPTIONAL { ?dataset dct:modified ?modified }\n  OPTIONAL { ?dataset dct:language ?language }\n  OPTIONAL { ?dataset dct:license ?license }\n  \n  # Distributions - Only basic properties\n  OPTIONAL {\n    ?dataset dcat:distribution ?distribution .\n    OPTIONAL { ?distribution dct:title ?distTitle }\n    OPTIONAL { ?distribution dct:description ?distDescription }\n    OPTIONAL { ?distribution dcat:downloadURL ?downloadURL }\n    OPTIONAL { ?distribution dcat:accessURL ?accessURL }\n    OPTIONAL { ?distribution dcat:mediaType ?mediaType }\n    OPTIONAL { ?distribution dct:format ?format }\n    OPTIONAL { ?distribution dct:license ?distLicense }\n  }\n}\nLIMIT 50",
          "parameters": [
            {
              "name": "org_dir3",
              "label": "Código DIR3 del Organismo",
              "placeholder": "ej: E05068001",
              "required": true,
              "defaultValue": "E05068001"
            }
          ]
        },
